<%- model_class = Factura -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='factura'>
  <p>Fecha <%= @factura.fecha %></p>
  <p>Tipodoc <%= @factura.tipodoc_id %></p>
  <p>Numero documento <%= @factura.numero_documento %></p>
  <p>Nombres <%= @factura.nombres %></p>
  <p>Apellidos <%= @factura.apellidos %></p>
  <p>Cantidad <%= @factura.cantidad %></p>
  <p>Producto <%= @factura.producto_id %></p>
  <p>Valor <%= @factura.valor %></p>
</div>
