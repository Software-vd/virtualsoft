<%- model_class = Tipodoc -%>
<center>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<center><dl class="dl-horizontal"></center>
  <dt><strong><%= model_class.human_attribute_name(:Sigla) %></strong></dt>
  <br>
  <dd><%= @tipodoc.Sigla %></dd>
  <br>
  <dt><strong><%= model_class.human_attribute_name(:cliente_id) %></strong></dt>
  <br>
  <dd><%= @tipodoc.cliente_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.Atras")),
              tipodocs_path, :class => 'btn btn-inverse'  %>
  <%= link_to t('.edit', :default => t("helpers.links.Editar")),
              edit_tipodoc_path(@tipodoc), :class => 'btn btn-info' %>
  <%= link_to t('.destroy', :default => t("helpers.links.Eliminar")),
              tipodoc_path(@tipodoc),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Esta seguro?')) },
              :class => 'btn btn-danger' %>
</div>
</center>