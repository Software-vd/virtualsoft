<%- model_class = Cotizacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='cotizacion'>
  <p><%= @cotizacion.id %></p>
  <p><%= @cotizacion.fecha %></p>
  <p><%= @cotizacion.tipodoc_id %></p>
  <p><%= @cotizacion.numero_documento %></p>
  <p><%= @cotizacion.nombres %></p>
  <p><%= @cotizacion.apellidos %></p>
  <p><%= @cotizacion.producto_id %></p>
  <p><%= @cotizacion.valor %></p>
</div>