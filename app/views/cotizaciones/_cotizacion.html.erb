<tr id=<%= "cotizacion_#{cotizacion.id}" %>>
 <td><%= link_to cotizacion.id, edit_cliente_cotizacion_path(@cliente,cotizacion), :remote => true %></td>
 <td><%= cotizacion.ciudad %></td>
 <td><%= cotizacion.fecha %></td>
 <td>
   <%= link_to image_tag('ver.gif', :title => "mostrar"), cliente_cotizacion_path(@cliente,cotizacion), :remote => true, :class => 'btn btn-mini btn-success' %>
  <%= link_to image_tag('editar.gif', :title => "editar"), edit_cliente_cotizacion_path(@cliente,cotizacion), :remote => true,
      :class => 'btn btn-mini btn-info' %>
  <%= link_to image_tag('eliminar.gif', :title => "eliminar"), cliente_cotizacion_path(@cliente,cotizacion), 
      :data => { :confirm => 'Esta Seguro?' },
      :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
   <%= link_to 'Detalles', cotizacion_cotizaciondetalles_path(cotizacion), :class => 'btn btn-inverse' %>
   <%= link_to 'Facturas', cotizacion_facturas_path(cotizacion), :class => 'btn btn-info' %>
 </td>
</tr>